@startuml
class Hospital {
  +hospitalId: String
  +name: String
}

class Office {
  +officeId: String
  +location: String
}

class Doctor {
  +doctorId: String
  +name: String
}

class Patient {
  +patientId: String
  +name: String
  +email: String
}

class Schedule {
  +startDateTime: DateTime
  +endDateTime: DateTime
}

class Appointment {
  +appointmentId: String
  +type: String
  +startDateTime: DateTime
  +endDateTime: DateTime
}

Hospital -- Office
Doctor -- Schedule
Office -- Schedule
Appointment -- Doctor
Appointment -- Patient
Appointment -- Office
@enduml