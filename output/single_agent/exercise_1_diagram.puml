@startuml
class UnderageUser {
  + fullName: String
  + studentCard: String
}
class AdultUser {
  + fullName: String
  + idCard: String
}
class Book {
  + title: String
  + author: String
}
class BorrowHistory {
  + borrowDate: Date
  + book: Book
}
UnderageUser -- Book : borrows
AdultUser -- Book : borrows
UnderageUser -- BorrowHistory : hasHistory
AdultUser -- BorrowHistory : hasHistory
BorrowHistory -- Book : refersTo
@enduml