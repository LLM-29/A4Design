@startuml
class Hospital {
  + name: String
  + address: String
}
class Office {
  + name: String
  + location: String
}
class Doctor {
  + name: String
  + specialty: String
}
class Patient {
  + name: String
  + patientId: String
}
class Appointment {
  + date: String
  + time: String
  + status: String
  + type: AppointmentType
  + duration: int
}
enum AppointmentType {
  CONSULTATION
  BLOOD_TEST
  SURGERY
}
Doctor --> Office
Hospital --> Office
Appointment --> Patient
Appointment --> Doctor
@enduml