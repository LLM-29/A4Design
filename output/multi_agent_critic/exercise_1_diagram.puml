@startuml

class UnderAgeUser {
    + fullName: String
    + studentCard: String
    + borrowingHistory: List<Borrowing>
    + maxBorrowedBooks: int
}

class AdultUser {
    + fullName: String
    + idCard: String
    + borrowingHistory: List<Borrowing>
    + maxBorrowedBooks: int
}

class Book {
    + title: String
    + author: String
}

class Borrowing {
    + borrowDate: Date
}

class Library {
    + name: String
}

UnderAgeUser --> Borrowing
AdultUser --> Borrowing
Borrowing --> Book
Library --> Book

@enduml